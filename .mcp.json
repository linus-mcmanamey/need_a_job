{
  "mcpServers": {
    "linkedin": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/stickerdaniel/linkedin-mcp-server",
        "linkedin-mcp-server",
        "--cookie",
        "li_at=AQEDAQGE4QMD5wSKAAABkKXB4q4AAAGaBREpZlYAPjbeBqD6SdoFPc6K7YY7d3D_mkEuJQBNIPDLt9sgnxAJFLYq9jfclEWE512dq7TmXBUkOkBx-3oVxSdA9E-QBZEQ0Fh8tJ5uWar_cpLRP-8vVuk8"
      ],
      "env": {
        "NO_PROXY": "localhost,127.0.0.1,::1",
        "no_proxy": "localhost,127.0.0.1,::1"
      }
    },
    "MCP_DOCKER": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run"
      ],
      "type": "stdio"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "executeautomation-playwright-server": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "playwright-server": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "ios-simulator": {
      "command": "npx",
      "args": [
        "-y",
        "ios-simulator-mcp"
      ]
    },
    "just-mcp": {
      "command": "/path/to/just-mcp",
      "args": [
        "--stdio"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
      }
    },
    "Figma Dev Mode MCP": {
      "url": "http://127.0.0.1:3845/mcp"
    }
  }
}
